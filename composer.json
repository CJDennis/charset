{
    "name": "cjdennis/charset",
    "description": "A library for character set functions",
    "type": "library",
    "require-dev": {
        "codeception/codeception": ">=2.4",
        "cjdennis/unit-test-compatibility": "^0.0.1"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "ext-iconv": "*",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {"CJDennis\\Charset\\": "src/CJDennis/Charset/"}
    },
    "autoload-dev": {
        "psr-4": {
            "CJDennis\\Charset\\": "tests-common/unit/CJDennis/Charset/",
            "Tests\\Codeception\\": "tests-codeception/",
            "Tests\\Common\\": "tests-common/",
            "Tests\\Phpunit\\": "tests-phpunit/"
        }
    }
}
